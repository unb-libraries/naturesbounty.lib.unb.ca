{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of naturesbounty.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {}
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "unb-libraries/naturesbounty.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "package": {
                "dist": {
                    "type": "file",
                    "url": "https://cdnjs.cloudflare.com/ajax/libs/jquery.colorbox/1.6.4/jquery.colorbox-min.js"
                },
                "name": "jackmoore/colorbox",
                "require": {
                    "composer/installers": "^1.6.0"
                },
                "type": "drupal-library",
                "version": "1.6.4"
            },
            "type": "package"
        }
    ],
    "require": {
        "drupal/bootstrap": "3.17",
        "drupal/config_update": "1.5",
        "drupal/colorbox": "1.4",
        "drupal/colorbox_inline": "1.1",
        "drupal/core": "8.7.1",
        "drupal/externalauth": "1.0",
        "drupal/features": "3.7",
        "drupal/google_analytics": "2.4",
        "drupal/ldap": "3.0-beta3",
        "drupal/libraries": "3.x.dev",
        "drupal/pathauto": "1.1",
        "drupal/redis": "1.0-rc2",
        "drupal/search_api": "1.7",
        "drupal/token": "1.0",
        "jackmoore/colorbox": "1.6.4",
        "unb-libraries/unb_lib_theme": "dev-8.x-bootstrap-3.x",
        "unb-libraries/drupal-builder": "^8.0.0"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
